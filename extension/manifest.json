{
  "manifest_version": 3,
  "name": "Feedbin Subscribe & Save",
  "version": "1.0",
  "description": "Subscribe to websites and save pages to read in Feedbin",
  "icons": {
    "32": "icons/dark-32.png",
    "64": "icons/dark-64.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Subscribe & Save"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-info-display@example.com",
      "strict_min_version": "109.0"
    }
  }
}